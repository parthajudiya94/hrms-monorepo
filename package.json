    "start:backend": "cd packages/backend && npm run dev"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "lerna": "^4.0.0"
  }
}
