{
  "name": "time-tracking-app",
  "version": "1.0.0",
  "description": "Time tracking App - Monorepo",
  "keywords": [],
  "license": "UNLICENSED",
  "author": "Parth Ajudiya",
  "private": true,
  "scripts": {
    "install": "lerna bootstrap --ignore-scripts",
